{
  "name": "New Tab FutureStack Countdown",
  "description": "Countdown time remaining until FutureStack 2022.",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
    "48": "extension-icons/Clock-48.png",
    "128": "extension-icons/Clock-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
  }
}
